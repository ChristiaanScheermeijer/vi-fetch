{
  "name": "vi-fetch",
  "version": "0.0.2",
  "description": "Easiest way to mock fetch",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*",
    "./matchers": {
      "types": "./global.d.ts"
    },
    "./setup": {
      "import": "./dist/setup.mjs",
      "require": "./dist/setup.js",
      "types": "./dist/global.d.ts"
    }
  },
  "scripts": {
    "test": "vitest --global --environment jsdom",
    "build": "tsup",
    "publish": "tsup && clean-publish"
  },
  "author": "sheremet-va",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.8",
    "clean-publish": "^3.4.5",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "tsup": "^5.11.10",
    "typescript": "^4.5.4",
    "vitest": "^0.0.127"
  },
  "dependencies": {
    "query-string": "^7.0.1",
    "tinyspy": "^0.2.6"
  },
  "clean-publish": {
    "cleanDocs": true,
    "packageManager": "pnpm"
  }
}