{
  "name": "vi-fetch",
  "version": "0.2.8",
  "description": "Easiest way to mock fetch",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/**",
    "global.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*",
    "./matchers": {
      "types": "./matchers.d.ts"
    },
    "./setup": {
      "import": "./dist/setup.js",
      "types": "./matchers.d.ts"
    }
  },
  "scripts": {
    "test": "vitest --global --environment jsdom",
    "build": "tsup",
    "publish": "clean-publish --clean-docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sheremet-va/vi-fetch.git"
  },
  "author": "sheremet-va",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sheremet-va/vi-fetch/issues"
  },
  "homepage": "https://github.com/sheremet-va/vi-fetch#readme",
  "devDependencies": {
    "@types/node": "^17.0.8",
    "clean-publish": "^3.4.5",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "tsup": "^5.11.10",
    "typescript": "^4.5.4",
    "vitest": "^0.0.127"
  },
  "dependencies": {
    "query-string": "^7.0.1",
    "tinyspy": "^0.2.6"
  }
}
